<h1>Listing papers</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>Year</th>
  </tr>
 
  <% @papers.each do |paper| %>
    <tr>
      <td><%= paper.title %></td>
      <td><%= paper.venue %></td>
      <td><%= paper.year %></td>
	  
	  <!--<td><%= link_to 'Show', author_path(author) %></td>
	  <td><%= link_to 'Edit', edit_author_path(author) %></td>
	  <td><%= link_to 'Destroy', author_path(author),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>-->
    </tr>
  <% end %>
</table>

