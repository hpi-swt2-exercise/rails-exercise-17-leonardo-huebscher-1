<h1>New Paper</h1>

<%= form_for :paper, url: papers_path do |f| %>
	<p>
		<%= f.label :title%><br>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :venue %><br>
		<%= f.text_field :venue %>
	</p>
	<p>
		<%= f.label :year %><br>
		<%= f.text_field :year %>
	</p>
	<% if @paper.errors.any? %>
		<% @paper.errors.full_messages.each do |msg| %>
			<%= msg %><br />
		<% end %>
	<% end %>
	<p>
		<%= f.submit %>
	</p>
<% end %>